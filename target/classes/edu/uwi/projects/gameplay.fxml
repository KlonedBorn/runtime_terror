<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vb_forground" prefHeight="622.0" prefWidth="900.0" spacing="10.0" style="-fx-alignment: center;" styleClass="root-container" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uwi.projects.GameplayController">
   <children>
      <Label fx:id="lb_playerName" styleClass="info-label" stylesheets="@css/global.css" text="&lt;player-name&gt;" />
      <AnchorPane fx:id="ap_display" prefHeight="485.0" prefWidth="879.0" VBox.vgrow="ALWAYS" />
      <Button fx:id="bt_next" mnemonicParsing="false" onAction="#onActionSwitchToNext" prefHeight="50.0" styleClass="menu-btn" text="Next" />
   </children>
   <stylesheets>
      <URL value="@css/menu.css" />
      <URL value="@css/global.css" />
   </stylesheets>
</VBox>
